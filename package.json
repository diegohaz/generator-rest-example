{
  "name": "generator-rest-example",
  "version": "0.0.0",
  "main": "src",
  "private": true,
  "scripts": {
    "start": "node .",
    "test": "cross-env NODE_ENV=test nyc ava",
    "test:unit": "npm test 'src/**/!(*.router*).test.js'",
    "test:integration": "npm test 'src/**/*.router.test.js'",
    "coverage": "nyc report -r lcov && opn coverage/lcov-report/index.html",
    "dev": "nodemon -i '*.test.js' .",
    "prod": "npm run build && nodemon -i '*.test.js' -r dotenv-safe/config dist",
    "lint": "eslint src/**/*.js",
    "clean": "del 'dist/!(.git*|Procfile)**'",
    "copy": "copy package.json dist && copy src/**/package.json dist/src",
    "transpile": "babel src -d dist/src -s inline -i 'src/**/*.test.js'",
    "build": "npm run clean && npm run copy && npm run transpile",
    "docs": "apidoc -i src -o docs && apidoc-markdown -p docs -o DOCS.md && opn docs/index.html"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime",
      "add-module-exports"
    ]
  },
  "ava": {
    "files": [
      "src/**/*.test.js"
    ],
    "require": [
      "babel-register"
    ],
    "concurrency": 5,
    "timeout": "2m",
    "babel": "inherit"
  },
  "devDependencies": {
    "apidoc": "^0.17.0",
    "apidoc-markdown": "^0.2.0",
    "ava": "^0.17.0",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "copy": "^0.3.0",
    "cross-env": "^3.0.0",
    "del-cli": "^0.2.0",
    "dotenv-safe": "^4.0.3",
    "eslint": "^3.3.1",
    "eslint-config-standard": "^6.0.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.0",
    "mockgoose": "^6.0.8",
    "nock": "^9.0.2",
    "nodemon": "^1.10.2",
    "nyc": "^10.0.0",
    "opn-cli": "^3.1.0",
    "sinon": "^1.17.5",
    "supertest": "^2.0.0",
    "supertest-as-promised": "^4.0.0"
  },
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "bcrypt": "^1.0.2",
    "bluebird": "^3.4.0",
    "body-parser": "^1.15.1",
    "bodymen": "^1.0.1",
    "compression": "^1.6.2",
    "cors": "^2.7.1",
    "crypto": "0.0.3",
    "express": "^4.13.4",
    "express-force-ssl": "^0.3.2",
    "jsonwebtoken": "^7.1.9",
    "lodash": "^4.13.1",
    "mongoose": "^4.4.19",
    "mongoose-create-unique": "^0.4.1",
    "mongoose-keywords": "^0.3.1",
    "morgan": "^1.7.0",
    "passport": "^0.3.2",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^2.1.0",
    "querymen": "^2.0.0",
    "rand-token": "^0.3.0",
    "request": "^2.74.0",
    "request-promise": "^4.1.1"  }
}
